          "_name": "header1",
          "_customHTML": "<section class=\"header1 slider\" group=\"Headers\" plugins=\"amp-main, amp-carousel\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline=\"\" title=\"Height Image\" name=\"heightImg\" min=\"400\" max=\"1200\" step=\"50\" value=\"700\" condition=\"!fullScreen\">         \n        <header>Text</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked=\"\">\n        <select title=\"Align\" name=\"alignTitle\" condition=\"showTitle || showSubtitle || showText || showButtons\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <header>Slider</header>\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Delay\" name=\"delay\" min=\"1000\" max=\"5000\" step=\"1000\" value=\"4000\" condition=\"autoplay\">\n        <input type=\"range\" min=\"2\" max=\"4\" step=\"1\" value=\"4\" title=\"Slides Amount\" name=\"imagesAmount\" required=\"\">\n        <input type=\"checkbox\" title=\"Padding Text\" name=\"showPadding\">\n        <input type=\"checkbox\" title=\"Carousel Button\" name=\"effectButton\" checked=\"\">\n        <header>Effect</header>   \n        <input type=\"checkbox\" title=\"Image Effect\" name=\"showEffect\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color 1\" name=\"overlayColor1\" value=\"#3b7eff\" condition=\"showEffect\">\n        <input type=\"color\" title=\"Overlay Color 2\" name=\"overlayColor2\" value=\"#7d3ed5\" condition=\"showEffect &amp;&amp; imagesAmount > '1'\">\n        <input type=\"color\" title=\"Overlay Color 3\" name=\"overlayColor3\" value=\"#fabd4a\" condition=\"showEffect &amp;&amp; imagesAmount >'2'\">\n        <input type=\"color\" title=\"Overlay Color 4\" name=\"overlayColor4\" value=\"#21ac13\" condition=\"showEffect &amp;&amp; imagesAmount >'3'\">\n        <input type=\"range\" inline=\"\" title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"showEffect\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    \n    <div class=\"container-fluid\">\n        <div class=\"slider-box\" data-autoplay=\"{{autoplay}}\" data-delay=\"{{delay}}\"><amp-carousel class=\"carousel\" height=\"9\" width=\"16\" layout=\"responsive\" type=\"slides\" loop=\"\"><div class=\"carousel-img mobirise-loader mbr-flex mbr-jc-c\">\n                    <div class=\"mbr-overlay\" mbr-if=\"showEffect\"></div>\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/bg6-1920x1280.jpg\" height=\"9\" width=\"13.5\" layout=\"responsive\" alt=\"Mobirise\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/bg6-1920x1280.jpg\" alt=\"Mobirise\" title=\"\">\n                    </amp-img>\n                    <div class=\"card-box container\" mbr-class=\"{'align-center': alignTitle == 2, 'align-right': alignTitle == 3}\">\n                        <h2 class=\"subtitle mbr-fonts-style mbr-white\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".subtitle\">Slider Subtitle</h2>\n                        <h1 class=\"title__text mbr-fonts-style mbr-bold mbr-white\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".title__text, .mbr-section-btn\">ADVANCES OF SCIENCE<br>MAKE EASIER LIFE</h1>\n                        <p class=\"mbr-text mbr-fonts-style mbr-white mbr-mt-1\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-section-btn, .mbr-text\">Labtech strives to integrate professionals with <br>companies and organizations around the world</p>\n                        <div mbr-buttons=\"\" class=\"mbr-section-btn\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove\" mbr-if=\"showButtons\"><a class=\"btn-primary btn btn-lg\" data-app-placeholder=\"Type Text\">\n                        Read More</a></div>\n                    </div>    \n                </div><div class=\"carousel-img mobirise-loader mbr-flex mbr-jc-c\" mbr-if=\"imagesAmount > 1\">\n                    <div class=\"mbr-overlay\" mbr-if=\"showEffect\"></div>\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/bg3-1920x1280.jpg\" height=\"9\" width=\"13.5\" layout=\"responsive\" alt=\"Mobirise\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/bg3-1920x1280.jpg\" alt=\"Mobirise\" title=\"\">\n                    </amp-img>\n                    <div class=\"card-box container\" mbr-class=\"{'align-center': alignTitle == 2, 'align-right': alignTitle == 3}\">\n                        <h2 class=\"subtitle mbr-fonts-style mbr-white\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".subtitle\">Slider Subtitle</h2>\n                        <h1 class=\"title__text mbr-fonts-style mbr-bold mbr-white\" mbr-class=\"{'mbr-pb-1': showText}\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".title__text, .mbr-section-btn\">ADVANCES OF SCIENCE\n<div>MAKE EASIER LIFE</div></h1>\n                        <p class=\"mbr-text mbr-fonts-style mbr-mt-1 mbr-white\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-section-btn, .mbr-text\">Labtech strives to